{
    "name": "devmonk/dm-fileman",
    "description": "File manager mainly for zf2-mailer / tinymce integration",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DevMonkCom/dm-common"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DevMonkCom/dm-test"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-mbstring": "*",
        "zendframework/zendframework": "2.2.*",
        "rwoverdijk/assetmanager": "1.3.*",
        "imagine/imagine": "0.6.*@dev",
        "devmonk/dm-common": "dev-master"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.2.*",
        "devmonk/dm-test": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "DmFileman\\": "src/"
        }
    }
}